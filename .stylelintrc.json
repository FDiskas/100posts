{
  "plugins": ["stylelint-prettier", "stylelint-scss"],
  "extends": ["stylelint-config-airbnb", "stylelint-config-standard", "stylelint-config-prettier"],
  "rules": {
    "prettier/prettier": true,
    "color-hex-length": "short",
    "selector-max-id": 2,
    "no-descending-specificity": null
  }
}
