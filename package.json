{
  "name": "100posts",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=en --hmr --open",
    "start:lt": "ng serve --configuration=lt --hmr --open",
    "build": "ng build",
    "build:prod": "ng build --prod --localize",
    "test": "ng test",
    "lint": "ng lint --fix",
    "e2e": "ng e2e",
    "format": "prettier --loglevel warn --write \"**/*.{ts,js,scss,md,html}\"",
    "test:watch": "jest --watch",
    "e2e:ci": "ng e2e --mode console",
    "int:extract": "ng xi18n --ivy"
  },
  "dependencies": {
    "@angular-builders/jest": "^10.0.1",
    "@angular/animations": "~10.1.2",
    "@angular/cdk": "^10.2.2",
    "@angular/common": "~10.1.2",
    "@angular/compiler": "~10.1.2",
    "@angular/core": "~10.1.2",
    "@angular/forms": "~10.1.2",
    "@angular/material": "^10.2.2",
    "@angular/platform-browser": "~10.1.2",
    "@angular/platform-browser-dynamic": "~10.1.2",
    "@angular/router": "~10.1.2",
    "@angular/service-worker": "~10.1.2",
    "@ngchile/cypress-cucumber-schematics": "^1.0.0-rc.7",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.2",
    "@angular-eslint/builder": "0.4.0-beta.2",
    "@angular-eslint/eslint-plugin": "0.4.0-beta.2",
    "@angular-eslint/eslint-plugin-template": "0.4.0-beta.2",
    "@angular-eslint/schematics": "^0.4.0-beta.2",
    "@angular-eslint/template-parser": "0.4.0-beta.2",
    "@angular/cli": "~10.1.2",
    "@angular/compiler-cli": "~10.1.2",
    "@angular/localize": "^10.1.2",
    "@angularclass/hmr": "^3.0.0",
    "@briebug/cypress-schematic": "^4.0.3",
    "@cypress/webpack-preprocessor": "5.4.5",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "3.9.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.2.0",
    "@typescript-eslint/parser": "3.9.0",
    "cypress-cucumber-preprocessor": "2.5.5",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^30.5.1",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "26.4.2",
    "jest-preset-angular": "8.3.1",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "protractor": "~7.0.0",
    "ts-loader": "8.0.4",
    "ts-node": "~8.3.0",
    "typescript": "~4.0.2"
  },
  "cypress-cucumber-preprocessor": {
    "step_definitions": "./e2e/step_definitions"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js}": "eslint --fix",
    "*.{js,json,scss,css,md,html}": "prettier --write"
  }
}
